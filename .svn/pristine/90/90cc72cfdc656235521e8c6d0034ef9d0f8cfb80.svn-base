.hy-gridpage .table-toolbar {
    overflow: visible;
}

.hy-gridpage .table-toolbar .commonSearch {
    z-index: 10;
    display: table;
}

.hy-gridpage .table-toolbar .commonSearch .commonSearchItem {
    /*display: inline;*/
    /*margin-right: 13px;*/
    display: table-cell;
    vertical-align: middle;
    padding-right: 20px;
}

.hy-gridpage .table-toolbar .commonSearch .commonSearchItem .k-checkbox-label, .hy-gridpage .table-toolbar .advancedSearchItem .k-checkbox-label {
    margin-right: 20px;
}

.hy-gridpage .table-toolbar .advancedSearchItem .k-checkbox-label {
    margin-top: 6px;
    /*float: left;*/
    /*display: inline-block;*/
    width: 19%;
}

.k-checkbox-label {
    padding-left: 20px;
    /*line-height: 6px;*/
    margin: 0;
    padding-bottom: 0;
    padding-top: 0;
}

.hy-gridpage .filterLabel, .hy-gridpage .filterInput {
    display: inline-block;
}

.hy-gridpage .advancedSearchItem .filterLabel {
    width: 28%;
    text-align: right;
    float: left;
}

.hy-gridpage .advancedSearchItem .filterInput {
    width: 68%;
}

.hy-gridpage .filterInput > input[type='text'] {
    height: 26px;
}

.hy-gridpage .advancedSearchItem .filterInput input[type='text'], .hy-gridpage .advancedSearchItem .filterInput .k-widget {
    width: 100%;
}

.hy-gridpage .table-toolbar .advancedSearch {
    z-index: 100000;
    display: none;
    position: absolute;
    background-color: #5b626b;
    right: 3px;
    width: 300px;
    /*border-radius: 2px;*/
    border: 1px solid #61666b;
    padding: 10px;
    top: 31px;
}

.hy-gridpage .table-toolbar .advancedSearch .advancedSearchItem {
    margin-bottom: 10px;
}

.hy-gridpage .table-toolbar .searchLayer {
    display: none;
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    /*background-color: red;*/
    z-index: 9;
}

.hy-gridpage .filterInput .k-autocomplete .k-input {

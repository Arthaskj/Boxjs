
body{
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}

.window-body, .absolute_block {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    overflow: hidden;
}


.window-content {
    position: absolute;
    padding: 5px;
    top: 0;
    left: 0;
    right: 0;
    bottom: 50px;
    overflow: auto;
    background-color: rgba(51, 51, 51, 0.1);
}

.window-footer {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    text-align: right;
    /*padding: 5px;*/
    border-width: 1px 0 0 0;
    border-style: solid;
    border-color: rgba(255, 255, 255, 0.125);
    background-color: rgba(51, 51, 51, 0.6);
}

.window-btn {
    margin: 5px 3px;
}


/*几个不同的颜色主题*/
.k-success {
    color: #fff;
    border-color: #4cb64c;
    border-bottom-color: #42a142;
    background: #5ebd5e;
    background-image: -webkit-linear-gradient(top, #62be62 0, #4cb64c 100%);
    background-image: linear-gradient(to bottom, #62be62 0, #4cb64c 100%);
    background-repeat: repeat-x;
}

.k-success:hover {
    color: #fff;
    border-color: #4E904E;
    background: #4E904E;
}

.k-success:focus {
    color: #fff;
}

.k-info {
    color: #fff;
    background-color: #4e85bd;
    border-color: #4278b0;
    /*background-image: -webkit-linear-gradient(top, #5fc2df 0, #46b8da 100%);*/
    /*background-image: linear-gradient(to bottom, #5fc2df 0, #46b8da 100%);*/
    /*background-repeat: repeat-x;*/
}

.k-info:hover {
    color: #fff;
    background-color: #3b6c9d;
    border-color: #315a83;
    -webkit-box-shadow: inset 0 1px 5px rgba(0, 0, 0, 0.08);
    box-shadow: inset 0 1px 5px rgba(0, 0, 0, 0.08);
}

.k-info:focus {
    color: #fff;
}

.k-warning {
    color: #fff;
    border-color: #f19a1f;
    border-bottom-color: #e38b0e;
    background: #f4b04f;
    background-image: -webkit-linear-gradient(top, #f4b254 0, #f3a537 100%);
    background-image: linear-gradient(to bottom, #f4b254 0, #f3a537 100%);
    background-repeat: repeat-x;
}

.k-warning:hover {
    color: #fff;
    border-color: #D29A49;
    background: #D29A49;
}

.k-warning:focus {
    color: #fff;
}

.k-danger {
    color: #fff;
    border-color: #e3503e;
    border-bottom-color: #df3823;
    background: #e66454;
    background-image: -webkit-linear-gradient(top, #e76858 0, #e3503e 100%);
    background-image: linear-gradient(to bottom, #e76858 0, #e3503e 100%);
    background-repeat: repeat-x;
}

.k-danger:hover {
    color: #fff;
    border-color: #C25F52;
    background: #C25F52;
}

.k-danger:focus {
    color: #fff;
}



.hy-notify {
    max-width: 220px;
    /*min-width: 220px;*/
    padding: 5px 10px;
    color: white;
    display: table;
}

.hy-notify.hy-notify-info {
    background-color: #1EA8FC !important;
}

.hy-notify.hy-notify-success {
    background-color: #5ebd5e !important;
}

.hy-notify.hy-notify-warning {
    background-color: #eca239 !important;
}

.hy-notify.hy-notify-error {
    background-color: #e66454 !important;
}

.hy-notify .hy-notify-icon {
    font-size: 28px;
    padding: 0 6px;
    vertical-align: middle;
    display: table-cell;
}

.hy-notify .hy-notify-icon.fa-warning {
    padding: 0 4px;
}

.hy-notify .hy-notify-title {
    font-weight: bold;
}

.hy-notify .hy-notify-message {
    width: 160px;
    -moz-word-break: break-all;
    -o-word-break: break-all;
    word-break: break-all;
    margin: 5px 0;
}

.hy-notify-2 {
    display: table;
    width: 100%;
}

.hy-notify-2.hy-notify-info-2 {
    color: #1EA8FC !important;
}

.hy-notify-2.hy-notify-success-2 {
    color: green !important;
}

.hy-notify-2.hy-notify-warning-2 {
    color: #E18400 !important;
}

.hy-notify-2.hy-notify-error-2 {
    color: red !important;
}

.hy-notify-2 .hy-notify-icon-2 {
    font-size: 28px;
    padding: 0 6px;
    vertical-align: top;
    text-align: center;
    padding-right: 12px;
    float: left;
}

.hy-notify-2 .hy-notify-message-2 {
    width: auto;
    word-break: break-all;
    padding-right: 10px;

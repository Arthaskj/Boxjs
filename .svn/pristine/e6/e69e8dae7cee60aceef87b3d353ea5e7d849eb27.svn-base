/*dropzone的样式*/

.dz-browser-not-supported .dz-message {
    display: none !important;
}

.dz-browser-not-supported.dropzone-box {
    min-height: auto !important;
    border: none !important;
    border-radius: 0 !important;
    width: auto !important;
    cursor: default !important;
    padding: 0 !important;
    -webkit-transition: none;
    transition: none;
}

.dropzone-box {
    position: relative;
    /*min-height: 260px;*/
    /*border: 3px dashed #ddd;*/
    /*border-radius: 3px;*/
    vertical-align: middle;
    width: 100%;
    height: 100%;
    cursor: pointer;
    /*padding: 0 15px 15px 0;*/
    -webkit-transition: all .2s;
    transition: all .2s;
}

.dz-default.dz-message {
    color: white;
    font-size: 20px;
    font-weight: 600;
    display: block;
    min-height: 70px;
    width: 300px;
    position: absolute;
    padding-left: 90px;
    padding-top: 12px;
    line-height: 22px;
    top: 50%;
    margin-top: -35px;
    left: 50%;
    margin-left: -150px;
    -webkit-transition: all .2s;
    transition: all .2s;
}

.dz-text-small {
    display: block;
    font-size: 14px;
    font-weight: 400;
    margin-top: 5px;
}

.dropzone-box .fa.fa-cloud-upload {
    display: block;
    width: 70px;
    height: 70px;
    position: absolute;
    left: 0;
    top: 0;
    text-align: center;
    line-height: 70px;
    font-size: 32px;
    /*background: #eee;
    color: #aaa;*/
    /*border-radius: 2px;*/
    -webkit-transition: all .2s;
    transition: all .2s;
}

.dropzone-box:hover .fa.fa-cloud-upload {
    color: #fff;
}

.dz-started .dz-default.dz-message {
    display: none;
}

.dz-started:hover .fa.fa-cloud-upload {
    background: #eee !important;
    color: #aaa !important;
}

.dz-preview {
    width: 180px;
    padding: 6px 10px 10px;
    margin: 15px 0 0 15px;
    display: inline-block;
    cursor: default;
    border: 1px solid #ddd;
    /*border-radius: 2px;*/
    vertical-align: top;
}

.dz-filename {
    color: white;
    padding-bottom: 2px;
    overflow: hidden;
}

.dz-filename span {
    height: 20px;
    line-height: 20px;
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    width: 158px;
}

.dz-size {
    padding-bottom: 8px;
}

.dz-thumbnail {
    width: 158px;
    height: 120px;
    text-align: center;
    display: table-cell;
    vertical-align: middle;
    overflow: hidden;
}

.dz-thumbnail > img {
    max-width: 100%;
    max-height: 120px;
}

.dz-preview .progess-bar, .dz-preview .progress {
    height: 10px;
}

.dz-preview .progress {
    margin: 0 -11px -11px -11px;
}

.dz-size {
    color: #aaa;
    font-size: 11px;
}

.dz-size span {
    font-weight: 600;
}

.dz-remove {
    line-height: 30px;
    display: block;
    margin: 11px -11px -11px -11px;
    text-align: center;
    color: white;
    border-top: 1px solid #ddd;
    border-bottom: 1px solid transparent;
    font-weight: 600;
    font-size: 10px;
    text-transform: uppercase;
    -webkit-transition: all .1s;
    transition: all .1s;
    border-bottom-right-radius: 2px;
    border-bottom-left-radius: 2px;
}

.dz-remove:hover {
    background: #e66454;
    color: #fff;
    border-top-color: transparent;
    text-decoration: none;
}

.dz-thumbnail-wrapper {
    margin: 0 -10px;
    padding: 10px;
    background: #4d5259;
    border: 1px solid #ddd;
    border-left: none;
    border-right: none;
    position: relative;
}

.dz-nopreview {
    text-transform: uppercase;
    font-size: 11px;
    font-weight: 600;
    color: #888;
}

.dz-thumbnail img[src] + .dz-nopreview {
    display: none;
}

.dz-error-mark, .dz-error-message, .dz-success-mark {
    display: none;
    position: absolute;
    left: -1px;
    right: -1px;
    top: -1px;
    bottom: -1px;
    text-align: center;
    /*background: rgba(0, 0, 0, .5);*/
    background: rgba(0, 0, 0, .1);
    box-shadow: 0 0 30px rgba(0, 0, 0, .1) inset;
    -webkit-transition: all .1s;
    transition: all .1s;
}

.dz-error-mark .fa, .dz-success-mark .fa {
    line-height: 140px;
    font-size: 50px;
    font-weight: 400;
}

.dz-error-mark .fa {
    color: #fa6b59;
}

.dz-success-mark .fa {
    color: #3690e6;
}

.dz-error-message {
    color: #FA0202;
    font-weight: 600;
    padding: 20px 10px 0;
}

.dz-error .dz-error-mark {
    display: block;
}

.dz-error .dz-error-message, .dz-error:hover .dz-error-mark {
    display: none;
}

.dz-error:hover .dz-error-message, .dz-success .dz-success-mark {
    display: block;
}


.hy-dropdownselect-container {

    position: fixed;

    display: none;
    overflow: hidden;
    background-color: transparent;
    /*padding: 2px;*/
    z-index: 99999;
    min-width: 240px;

}

.hy-dropdownselect-container ul {
    margin: 0;
    list-style: none;
}

ul.dropdownselectroot {
    padding-left: 5px;
    padding-right: 5px;
    border: 1px solid #6f6f6f !important;
    position: relative;
    /*transition-duration: 0.2s;*/
    background: #4a4f53;
    width: 100%;
    overflow-y: auto;
    max-height: 300px;
}

ul.dropdownselectroot ul {
    padding-left: 10px;
}

ul.dropdownselectroot li {
    padding: 2px 0;
    white-space: nowrap;
}

ul.dropdownselectroot .k-checkbox-label {
    display: block;
    /*padding: 5px 4px 4px 15px;*/
}

ul.dropdownselectroot .k-checkbox-label:hover:after, ul.dropdownselectroot .k-checkbox:checked + .k-checkbox-label:hover:after {
    border-color: #66afe9;
}

ul.dropdownselectroot label:hover {
    background-color: #3e4246;
}

ul.dropdownselectroot .k-checkbox-label:after {
    margin-top: 2px;
}

ul.dropdownselectroot .k-checkbox-label .collapse-sign {
    vertical-align: top;
    display: inline-block;
}

ul.dropdownselectroot label {
    font-weight: normal;
    margin-bottom: 0;
    display: block;
}

ul.dropdownselectroot .item-title, ul.dropdownselectroot .group-title, ul.dropdownselectroot .all-title, ul.dropdownselectroot .all-info, ul.dropdownselectroot .group-info {
    padding-left: 4px;
    display: inline-block;
    overflow: hidden;
    max-width: 100%;
    text-overflow: ellipsis;
}

ul.dropdownselectroot .dropdownselect-group {
    /*border-bottom: 1px solid #ddd;*/
    cursor: pointer;
    display: block;
}

ul.dropdownselectroot .k-list-filter {
    padding: 0;
    min-height: calc(1.75rem + 1.5em);
}

ul.dropdownselectroot .k-list-filter > .k-icon {
    right: 0.75rem;
}

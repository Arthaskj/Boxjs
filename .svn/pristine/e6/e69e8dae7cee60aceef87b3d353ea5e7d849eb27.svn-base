.hy-panel.hy-panel-max {
    position: fixed;
    top: 0px;
    left: 1px;
    right: 1px;
    z-index: 50000000;
    bottom: -20px;
}

.hy-panel .hy-panel-header {
    /*border-bottom: 1px solid #e7e7e7;*/
    /*background-color: rgba(51,51,51,0.425);*/
    height: 46px;
    color: white;
}

.hy-panel .hy-panel-header .hy-panel-title {
    padding: 15px;
    /*font-weight: bold;*/
    float: left;
    font-size: 14px;
}

.hy-panel .hy-panel-header .hy-panel-title .hy-panel-title-text {
    border-left: 5px #f0af03 solid;
    padding-left: 8px;
    display: inline-block;
}

.hy-panel .hy-panel-header .hy-panel-actions, .hy-panel .hy-panel-header .hy-panel-buttons {
    float: right;
    padding: 10px;
}

.hy-panel .hy-panel-header .hy-panel-buttons .hy-panel-btn {
    display: inline-block;
    margin-bottom: 0;
    font-weight: normal;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
    background-image: none;
    border: 1px solid transparent;
    white-space: nowrap;
    line-height: 1.428571429;
    -webkit-user-select: none;
    text-decoration: none;
    margin-top: 3px;
    margin-right: 3px;
    font-size: 12px !important;
    padding: 1px 8px !important;
    -webkit-tap-highlight-color: #e53b2c;
    /*-webkit-border-radius: 2px;
    -moz-border-radius: 2px;*/
    box-shadow: inset 0 -2px 0 rgba(0, 0, 0, 0.05);
}

.hy-panel .hy-panel-header .hy-panel-actions .hy-panel-btn {
    padding: 1px 8px;
    border-radius: 4px;
    color: rgba(255, 255, 255, 0.45);
    -webkit-transition: color 0.15s ease-in-out;
    -o-transition: color 0.15s ease-in-out;
    transition: color 0.15s ease-in-out;
}

.hy-panel .hy-panel-header .hy-panel-actions .hy-panel-btn span:hover {
    color: rgba(255, 255, 255, 0.6);
}

.hy-panel .hy-panel-content {
    padding: 5px;
}

.hy-panel.hy-panel-max .hy-panel-content {
    height: calc(100% - 46px);
}

.hy-panel.hy-panel-max {
    background-image: url(../imgs/bg-pattern.svg), -webkit-radial-gradient(10% 0, farthest-side ellipse, #414e60 20%, #58646f);
    background-size: cover;
    background-attachment: fixed, fixed;
}




.cash-wrap {

}

.cash-wrap h6 {
    cursor: pointer;
    margin: 2px 0
}

.cash-wrap table {
    table-layout: fixed;
}

.cash-wrap .cash-day-body-header {
    background: #F1F1F1;
}

.cash-wrap .cash-day-body .in {
    color: #FF476E;
}

.cash-wrap .cash-day-body .out {
    color: #008000;
}

.cash-wrap .cash-day-body td {
    background: #fff;
    border-left: none;
    border-right: none;
}

.cash-wrap .cash-day-body td.empty {
    border: none;
}

.cash-wrap .cash-day-body .total td {

}

.cash-wrap .cash-day-body td:nth-child(3),
.cash-wrap .cash-day-body td:nth-child(6) {
    border-right: 1px solid #ddd;
}

.check-wrap table {
    margin-bottom: 0;
}

.check-wrap table td {
    background: #fff;
    color: #777;
}

.check-wrap table td.title {
    font-weight: bold;
    color: #555;
    background: rgba(0, 0, 0, .02);
}

.check-left {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    width: 500px;
    border-right: 1px solid #dddddd;
    padding: 10px;
    overflow: auto;
}

.check-left h4 {
    margin-bottom: 0;
}

.check-left hr, .check-right hr {
    margin: 12px -10px;
    border-style: dashed;
}

.check-right hr {
}

.check-left .request-item-header {
    margin: -10px;
    padding: 10px 10px 1px 10px;
}

.check-left .request-item-header p {
    margin-bottom: 15px;
}

.check-left .request-item-header p:last-child {
    margin-bottom: 10px;
}

.check-left .request-item-checked {
    text-align: center;
    font-size: 17em;
    margin: 0 auto;
    position: absolute;
    top: 150px;
    left: 140px;
    z-index: 1;
    opacity: 0.5;
    color: red;
    transform: rotate(-30deg);
    -ms-transform: rotate(-30deg);
    -moz-transform: rotate(-30deg);
    -webkit-transform: rotate(-30deg);
    -o-transform: rotate(-30deg);
}

.check-left .request-item-bond {
    margin-bottom: 10px;
}

.check-right {
    position: absolute;
    top: 0;
    left: 500px;
    bottom: 0;
    right: 0;
    padding: 10px;
    overflow: auto;
}

.check-right .panel {
    margin-bottom: 10px;
    border-color: #f1f1f1;
    -webkit-box-shadow: 1px 1px 1px 0 #ddd;
    -moz-box-shadow: 1px 1px 1px 0 #ddd;
    box-shadow: 1px 1px 1px 0 #ddd;
}

.check-right .panel .panel-body {
    padding: 10px;
}

.check-right .panel .well {
    padding: 10px;
    margin-bottom: 10px;
}

.check-right .panel .well:last-child {
    margin-bottom: 0;
}

.check-right .trade-item-header {
    position: relative;
}

.check-right .trade-item-header p {
    margin-bottom: 15px;
}

.check-right .relevance {
    display: none;
    margin-top: 10px;
    color: #ff0000;
}

.check-right .relevance a {
    color: #ff0000;
}

.check-right .trade-item-title {
    display: inline-block;
    margin-bottom: 0;
    min-height: 15px;
}

.check-right .trade-item-actions {
    display: none;
    margin-left: 15px;
}

.check-right .trade-item-checked .trade-item-actions {
    display: inline-block;
}

.check-right .trade-item-checked .checkbox {
    padding-left: 0;
}

.check-right .trade-item-checked .relevance {
    display: block;
}

.check-right .trade-item-checked .checkbox .lbl:before,
.check-right .trade-item-checked .checkbox .lbl:after,
.check-right .trade-item-checked .checkbox input {
    display: none !important;
}

.parameter-list {
    margin: 0;
    padding: 0;
}

.parameter-list li {
    list-style: none;
    float: left;
    margin: 0 10px 7px 0;
    border: 1px solid #ddd;
    background: #FFF;
    min-width: 200px;
    border-radius: 3px;
}

.parameter-list li hr {
    margin: 0;
    border-top-style: dashed;
}

.parameter-list li .name {
    margin: 0;
    padding: 10px;
    color: #999;
    font-weight: bold;
    cursor: pointer;
    background: #f3f3f3;
}

.parameter-list li .comment {
    padding: 10px;
    margin: 0;
}

.parameter-list li i {
    margin-left: 5px;
    display: none;
}

.parameter-list li .name:hover i {
    display: inline-block;
}

.parameter-setting {

}

.parameter-setting table {
    table-layout: fixed;
}

.parameter-setting table td {
    padding: 7px 8px !important;
    border-width: 1px !important;
}

.parameter-setting td.editable {
    padding: 0 !important;
    background-color: #FFFFE1;
    overflow: visible;
}

.parameter-setting td.editable.has-error > input,
.parameter-setting td.editable.has-error .parameter-edit a,
.parameter-setting td.editable.has-error .parameter-edit input {
    background: repeat-x 0 0;;
    background-position: bottom;
    background-image: url(../images/invalid_line.gif);
    border-color: transparent !important;
}

.parameter-setting td.editable input,
.parameter-setting td.editable .input-group-addon {
    margin: 0;
    border: 1px solid #FFFFE1;
    background-color: #FFFFE1;
    border-radius: 0;
}

.parameter-setting td.editable .input-group-addon {
    border-left: 1px solid #ddd;
}

.parameter-setting td.editable .input-group-addon > .fa {
    font-size: 12px;
}

.parameter-setting td.editable .input-group {
    margin: 0;
}

.parameter-setting td.editable .select2-container {
    background: #FFFFE1;
}

.parameter-setting td.editable .select2-choice {
    background: #FFFFE1;
    border-color: #FFFFE1;
}

.parameter-setting td.editable .select2-choice .select2-arrow {
    border-left: 1px solid #ddd;
    width: 30px;
}



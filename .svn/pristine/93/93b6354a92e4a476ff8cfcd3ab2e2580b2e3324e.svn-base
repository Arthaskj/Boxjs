    /*width: 200px;*/
    color: #666;
    display: inline-block;
    font-size: 15px;
    font-weight: 200;
    line-height: 30px;
    vertical-align: text-top;
}


.content-wrapper .page-header .page-assistant-back-btn {
    display: none;
    overflow: hidden;
}

.content-wrapper .page-header .checkbox-inline,
.content-wrapper .page-header .radio-inline,
.content-wrapper .page-header .checkbox,
.content-wrapper .page-header .radio {
    line-height: 25px;
}

.content-wrapper .page-header .form-group .checkbox,
.content-wrapper .page-header .form-group .radio {
    margin-right: 10px;
}

.content-wrapper .page-header .form-group .checkbox:last-child,
.content-wrapper .page-header .form-group .radio:last-child {
    margin-right: 0;
}

.gt-ie8 .content-wrapper .page-header .radio .lbl:before,
.gt-ie8 .content-wrapper .page-header .radio .lbl:after,
.gt-ie8 .content-wrapper .page-header .checkbox .lbl:before,
.gt-ie8 .content-wrapper .page-header .checkbox .lbl:after,
.gt-ie8 .content-wrapper .page-header .radio-inline .lbl:before,
.gt-ie8 .content-wrapper .page-header .radio-inline .lbl:after,
.gt-ie8 .content-wrapper .page-header .checkbox-inline .lbl:before,
.gt-ie8 .content-wrapper .page-header .checkbox-inline .lbl:after{
    margin: 5px 3px -3px -20px !important;
}

.content-wrapper .page-action-wrap {
    /*margin-left: 200px;*/
    text-align: right;
}

.content-wrapper .page-action-wrap .form-inline > * {
    margin-right: 15px;
    font-size: 12px;
    color: #829091;
}

.content-wrapper .page-action-wrap .form-inline a {
    text-decoration: none;
}

.content-wrapper .page-action-wrap .form-inline > *:last-child {
    margin-right: 0;
}

.content-wrapper .page-action-wrap .form-inline > .btn {
    color: #333;
}

.content-wrapper .page-action-wrap .form-group:last-child {
    margin-bottom: 0;
}

.content-wrapper .page-header .page-header-icon {
    font-size: 19px;
    line-height: 20px;
    display: none;
    vertical-align: middle;
    margin-right: 3px;
    color: #999;
}

.content-wrapper .page-header .page-header-title {
    vertical-align: middle;
}

.content-wrapper .page-header .page-header-search {
    vertical-align: middle;
    font-size: 20px;
    margin-left: 10px;
    cursor: pointer;
    color: #666;
}

.content-wrapper .page-header .page-header-search:hover {
    color: #000000;
}

.content-has-header .page-header {
    display: block !important;
}

.content-wrapper .page-body, .content-wrapper .page-body .page-item, .content-wrapper .page-body .page-content {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    overflow: hidden;
}

.content-wrapper .page-body .page-content {
    padding: 10px;
    overflow: auto;
}

.content-has-header .page-body {
    top: 47px !important;
}

.content-wrapper .page-body .page-item .page-action-bar .item {
    margin-left: 5px !important;
}

.content-wrapper .page-body .page-item .page-action-bar span {
    margin-right: 5px;
}

.content-wrapper .page-body .page-item-has-actions .page-action-bar{
    display: block !important;
}

.content-wrapper .page-body .page-item-has-actions .page-content {
    bottom: 50px !important;
}


.content-wrapper .page-body > .iframe {
    width: 100%;
    height: 100%;
    margin: 0;
    position: relative;
}

.content-wrapper .page-body > .iframe > iframe {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    border: none;
}

.content-wrapper .page-assistant-body {
    display: none;
    z-index: 1000;
}

.has-assistant-page .content-wrapper .page-body {
    display: none;
}

.has-assistant-page .content-wrapper .page-assistant-body {
    display: block;
}

.has-assistant-page .content-wrapper .page-header .page-assistant-back-btn {
    display: block !important;
}

.has-assistant-page .content-wrapper .page-header .page-search-body {
    display: none !important;
}


.search-wrap .radio,
.search-wrap .checkbox {
    line-height: 26px;
    margin-right: 10px;
}

.search-wrap .form-group .radio:last-child,
.search-wrap .form-group .checkbox:last-child {
    margin-right: 0;
}

.search-wrap .form-inline > * {
    margin-right: 10px;
}

.search-wrap .form-inline > *:last-child {
    margin-right: 0;
}


.gt-ie8 .search-wrap .radio .lbl:before,
.gt-ie8 .search-wrap .radio .lbl:after,
.gt-ie8 .search-wrap .checkbox .lbl:before,
.gt-ie8 .search-wrapr .checkbox .lbl:after  {
    margin: 6px 3px -3px -20px !important;
}

.search-wrap .checkbox input.px + .lbl:after {
    top: 6px !important;
}


.window-super-module-box .super-module-list {
    list-style: none;
    padding: 10px;
    margin: 0;
}

.window-super-module-box .super-module-list .item {
    list-style: none;
    display: inline-block;
    margin: 0 0 20px 0;
    width: 178px;
    height: 178px;
    cursor: pointer;
    text-align: center;
    border: 1px solid transparent
}

.window-super-module-box .super-module-list .item:hover {
    -webkit-transition: background 1s;
    transition: background 1s
}

.window-super-module-box .super-module-list .item .icon {
    margin: 5px auto;
    outline: none;
}

.window-super-module-box .super-module-list .item .icon i {
    font-size: 10em;
    color: #999
}

.window-super-module-box .super-module-list .item .text {
    display: block;
    width: 178px;
    height: 22px;
    line-height: 22px;
    text-align: center;
    font-size: 11px;
    position: relative;
    padding: 0 5px;
    outline: none;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    word-break: break-all;
}






.dropdown-menu .load {
    display: none;
}

.dropdown-menu.loading .load {
    display: block;
    text-align: center;
    color: #AAA;
    padding: 5px 0;
}

.dropdown-menu-shortcut {

}

.dropdown-menu-utils {
    min-width: 432px;
    padding: 5px !important;
}

.dropdown-menu-utils .item {
    list-style: none;
    float: left;
    margin: 0 5px 0 0;
    width: 80px;
    height: 80px;
    cursor: pointer;
    text-align: center;
    border: 1px solid transparent !important;
}

.dropdown-menu-utils .item .icon {
    width: 40px;
    height: 40px;
    margin: 5px auto;
    outline: none;
}

.dropdown-menu-utils .item .icon i {
    width: 40px;
    height: 40px;
    line-height: 40px;
    vertical-align: middle;
    display: inline-block;
    font-size: 40px;
}

.dropdown-menu-utils .item .icon img {
    width: 40px;
    height: 40px;
}

.dropdown-menu-utils .item .text {
    display: block;
    width: 80px;
    height: 22px;
    line-height: 22px;
    text-align: center;
    color: #666;
    font-size: 11px;
    position: relative;
    padding: 0 5px;
    outline: none;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    word-break: break-all;
}

.dropdown-menu-utils .item:nth-child(5n + 0) {
    margin-right: 0;
}

.dropdown-menu-utils .item:hover {
    border-radius: 2px;
}

.dropdown-menu-utils .item:hover .text {
    color: #FFF;
}



.setting_window_inner {
    margin: -10px;
    overflow: hidden
}

.setting_window_inner .setting-tab {
    margin-top: 10px;
    padding: 0 10px;
}

.setting_window_inner .setting-tab-content {
    padding: 10px;
    overflow: auto
}

.setting-tab-content .theme-list .theme-item {
    position: relative;
    width: 200px;
    margin-right: 10px;
    cursor: pointer;
    -webkit-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
}

.setting-tab-content .theme-list .theme-item .thumbnail {
    margin-bottom: 10px;
    padding: 1px;
}

.setting-tab-content .theme-list .theme-item .desc {
    display: none;
    position: absolute;
    top: 1px;
    bottom: 11px;
    left: 1px;
    right: 1px;
    -webkit-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
    background: transparent;
    text-align: center;
    line-height: 230px;

}

.setting-tab-content .theme-list .theme-item .desc i {
    font-size: 5em;
    color: #67cea6;
}

.setting-tab-content .theme-list .theme-item-active .thumbnail{
    opacity: 0.7;
}

.setting-tab-content .theme-list .theme-item-active .desc{
    display: block;

}

.setting-tab-content .theme-list .theme-item .thumbnail .image {
    height: 200px;
    width: 200px;
    display: block;
}

.todo-group-title {
    margin: 0;
    line-height: 31px;
    padding: 0 0 0 10px;
    background: #fafafa;
    border-bottom: 1px solid #e7e7e7;
    border-top: 1px solid #f4f4f4;
    color: #999
}

.todo {
    margin: 0;
    padding: 0;
    min-height: 5px;
    list-style: none
}

.todo>li {
    display: block;
    position: relative;
    /*overflow: hidden;*/
    border-bottom: 1px solid #e7e7e7;
    margin: 0 5px;
    /*background: url(data:image/png;base64,) 1px 11px no-repeat*/
}

.todo>li:last-child,.todo>li:only-child {
    border-bottom: none
}

.todo>li>:first-child {
    display: block;
    /*border-right: 1px solid #FFE1EB;*/
    height: 100%;
    padding: 6px 11px 6px 18px;
    /*width: 20px;*/
    vertical-align: top;
    position: absolute
}

.todo>li>:first-child:hover {
    /*cursor: move*/
}

.todo>li.complete>:first-child:hover {
    /*cursor: default*/
}

.todo>li.complete {
    background: 0 0
}

.todo>li>p {
    height: 100%;
    margin-left: 52px;
    border-left: 1px solid #FFACAC;
    /*display: inline-block;*/
    padding: 8px 0 6px 7px;
    margin-bottom: 0;
    min-height: 37px;
    line-height: normal;
    font-size: 14px;
    font-weight: 500;
    color: #333
}

.todo>li>p>span {
    display: block;
    line-height: 12px;
    font-size: 10px;
    font-weight: 400
}

.todo>li>p>span.rightstatus {
    float: right;
    line-height: 20px;
    color: #999;
}

.todo>li>p>:first-child {
    margin-top: -5px;
    color: #999;
    margin-bottom: 4px
}

.todo>li>p>.date {
    color: #bfbfbf
}

.todo>li.complete>* {
    /*text-decoration: line-through;
    font-style: italic*/
}

.todo>li.ui-sortable-helper {
    border-top: 1px solid #eee;
    background: rgba(113,132,63,.1)
}

.todo-msg-a-left{
    display: inline-block;
    float: left;
}

.todo-msg-a-right{
    display: inline-block;
    float: right;
}
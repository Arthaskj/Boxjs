.scroll::-webkit-scrollbar {
    -webkit-overflow-scrolling: touch;
    height: 5px;
    width: 5px;
    -webkit-border-radius: 5px;
    background-color: #fafafa;
}

.scroll-no-width::-webkit-scrollbar {
    width: 0;
    height: 0;
}

.scroll-big-width::-webkit-scrollbar {
    height: 8px;
    width: 8px;
}

.scroll::-webkit-scrollbar:hover {
    background-color: #E9E9E9;
    border: 1px solid #dbdbdb;
}

.scroll::-webkit-scrollbar-button:start:decrement,
.scrollable::-webkit-scrollbar-button:end:increment {
    background: transparent;
    display: block;
    height: 0;
}

.scroll::-webkit-scrollbar-track {
    background-clip: padding-box;
    border-width: 0 0 0 4px;
    border: solid transparent;
}

.scroll::-webkit-scrollbar-track-piece {
    -moz-border-radius: 0;
    -ms-border-radius: 0;
    -o-border-radius: 0;
    -webkit-border-radius: 0;
    background-color: transparent;
    border-radius: 0;
}

.scroll::-webkit-scrollbar-thumb {
    -moz-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, 0.1), inset 0 -1px 0 rgba(0, 0, 0, 0.07);
    -webkit-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, 0.1), inset 0 -1px 0 rgba(0, 0, 0, 0.07);
    background-clip: padding-box;
    background-color: rgba(0, 0, 0, 0.2);
    border-width: 0;
    border: none;
    box-shadow: inset 1px 1px 0 rgba(0, 0, 0, 0.1), inset 0 -1px 0 rgba(0, 0, 0, 0.07);
}

.scroll::-webkit-scrollbar-thumb:vertical,
.scrollable::-webkit-scrollbar-thumb:horizontal {
    -moz-border-radius: 0;
    -ms-border-radius: 0;
    -o-border-radius: 0;
    -webkit-border-radius: 0;
    background-color: #c6c6c6;
    border-radius: 0;
}

.scroll::-webkit-scrollbar-thumb:active {
    -moz-box-shadow: inset 1px 1px 3px rgba(0, 0, 0, 0.33);
    -webkit-box-shadow: inset 1px 1px 3px rgba(0, 0, 0, 0.33);
    background-color: rgba(0, 0, 0, 0.44);
    box-shadow: inset 1px 1px 3px rgba(0, 0, 0, 0.33);
}

.scroll::-webkit-scrollbar-thumb:hover {
    background-color: #959595;
}

.scroll::-webkit-scrollbar-button {
    width: 0;
    display: none;
}


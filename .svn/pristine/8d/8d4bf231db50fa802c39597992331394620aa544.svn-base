}
.uploadify-button  .uploadify-button-text {
    display: block;
    height: 32px;
    padding-left: 30px;
    line-height: 30px;
    font-weight: normal;
    text-indent: 0;
}
.uploadify-button.disabled {
    background-color: #D0D0D0;
    color: #808080;
}

.btn-primary-hover {
    color: #fff;
    background-color: #296191;
    border-color: #1f496d
}
.uploadify-queue {
    margin-bottom: 1em;
}
.uploadify-queue-item {
    background-color: #F5F5F5;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    font: 11px Verdana, Geneva, sans-serif;

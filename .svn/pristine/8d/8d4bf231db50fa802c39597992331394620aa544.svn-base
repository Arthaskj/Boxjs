.pa-flot-info i {
    position: absolute;
    display: block;
    content: '';
    width: 8px;
    height: 8px;
    top: 12px;
    left: 10px;
    border-radius: 9999px
}

.pa-flot-tooltip {
    position: absolute;
    display: none;
    padding: 5px 10px;
    font-size: 12px;
    background: #222;
    color: #fff;
    border-radius: 2px
}

.tickLabel {
    font-size: 12px;
    color: #666
}

.yAxis .tickLabel:first-child, .yAxis .tickLabel:last-child {
    display: none
}

.pieLabel > div {
    font-size: 12px !important
}

.slimScrollBar {
    width: 6px !important;
    opacity: .3 !important
}

.slimScrollRail {
    width: 6px !important
}

.window {
    position: absolute;
    top: -99999px;
    left: -99999px;
    z-index: 2000;
    background-color: #FFF;
    -webkit-animation-duration: .5s;
    -moz-animation-duration: .5s;
    -ms-animation-duration: .5s;
    animation-duration: .5s;
}

.window .header {
    padding: 0 15px 0 10px;
    height: 45px;
    line-height: 45px;
    opacity: 0.8;
    position: relative;
}

.window .header .title {
    font-size: 12px;
}

.window .header .tools {
    position: absolute;
    right: 0;
    top: 0;
    margin: 0;
    padding: 0;
    overflow: hidden;
}

.window .header .tools li {
    list-style: none;
    float: left;
    padding: 15px 10px;
    height: 45px;
    line-height: 45px;
    cursor: pointer;
    opacity: 0.9;
    filter: alpha(opacity=90);
    border: none;
}

.window .header .tools li i {
    display: block;
    width: 16px;
    height: 16px;
    background-image: url(../images/tool-sprites-fff.png);
    background-repeat: no-repeat;
}

.window .header .tools .refresh i {
    background-position: 0 -240px;
}

.window .header .tools .toggle i {
    background-position: 0 -256px;
}

.window .header .tools .toggle.max i {
    background-position: 0 -16px;
}

.window .header .tools .close:hover {
    border: none;
}

.window .header .tools .close i {
    background-position: 0 0;
}

.window .content {
    overflow: auto;
    border: 1px solid #ccc;
    border-top: none;
    border-bottom: none;
    position: relative;
}

.window .footer {
    display: block;
    text-align: right;
    padding: 5px;
    background: #f8f8f8;
    border: 1px solid #ccc;
    border-top: 1px solid #e5e5e5;
}

.window .footer .btn {
    margin: 0 0 0 5px;
}

.window .footer .btn .fa {
    margin-right: 5px;
}

.window .loader {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    opacity: 0.7;
    filter: alpha(opacity=70);
    background: transparent url(../images/page-loading.gif) no-repeat center;
}

.window-layer {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    /*cursor: no-drop;*/
    background-color: #FFF;
    opacity: 0.1;
    filter: alpha(opacity=10);
}

.window-resizable-helper {
    border: 1px dotted #333;
}

@-webkit-keyframes window-fadeIn-right {
    0% {
        opacity: 0.5;
        -webkit-transform: translateX(500px);
        transform: translateX(500px)
    }
    100% {
        opacity: 1;
        -webkit-transform: translateX(0);
        transform: translateX(0)
    }
}

@keyframes window-fadeIn-right {
    0% {
        opacity: 0.5;
        -webkit-transform: translateX(500px);
        -ms-transform: translateX(500px);
        transform: translateX(500px)
    }
    100% {
        opacity: 1;
        -webkit-transform: translateX(0);
        -ms-transform: translateX(0);
        transform: translateX(0)
    }
}

.window-fadeIn-right {
    -webkit-animation-name: window-fadeIn-right;
    animation-name: window-fadeIn-right
}

@-webkit-keyframes window-fadeOut-right {
    0% {
        opacity: 1;
        -webkit-transform: translateX(0);
        transform: translateX(0)
    }
    100% {
        opacity: 0.0;
        -webkit-transform: translateX(500px);
        transform: translateX(500px)
    }
}

@keyframes window-fadeOut-right {
    0% {
        opacity: 1;
        -webkit-transform: translateX(0);
        -ms-transform: translateX(0);
        transform: translateX(0)
    }
    100% {
        opacity: 0.0;
        -webkit-transform: translateX(500px);
        -ms-transform: translateX(500px);
        transform: translateX(500px)
    }
}

.window-fadeOut-right {
    -webkit-animation-name: window-fadeOut-right;
    animation-name: window-fadeOut-right
}

/*工作流样式*/
.wfWizard {
    display: block;
    list-style: none;
    padding: 0;
    position: relative;
    width: 100%;
}

.wfWizard a:hover, .wfWizard a:active, .wfWizard a:focus {
    text-decoration: none;
}

.wfWizard li {
    display: block;
    float: left;
    width: 25%;
    text-align: center;
    padding-left: 0;
    margin-top: 10px;
    margin-bottom: 30px;
}

.wfWizard li:before {
    background-color: #92a2a8;
    -webkit-box-shadow: inset 0 -1px 0 rgba(0, 0, 0, .15);
    box-shadow: inset 0 -1px 0 rgba(0, 0, 0, .15);
    font-weight: 700;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, .25);
    /*background-image: url(../img/pattern/overlay-pattern.png);*/
    height: 10px;
    line-height: 15px;
    content: "";
    display: block;
    font-size: 0;
    overflow: hidden;
    position: relative;
    top: 30px;
    left: 1px;
    width: 100%;
    z-index: 1;
    margin-left: 50%;
}

.wfWizard li:first-child:before {
    right: 50%;
    max-width: 100%;
}

.wfWizard li:last-child:before {
    max-width: 0;
    width: 50%;
}

.wfWizard li.complete .step {
    background: #559c63;
    padding: 10px 16px;
    border: 3px solid #55606E;
}

.wfWizard li.done:before {
    background: #559c63;
}

.wfWizard li.future:before {
    background: #92a2a8;
}

.wfWizard li .step i {
    font-size: 10px;
    font-weight: normal;
    position: relative;
    top: -1.5px;
}

.wfWizard li .step {
    background: #B2B5B9;
    color: #FFF;
    display: inline;
    font-size: 15px;
    font-weight: bold;
    padding: 10px 16px;
    border: 3px solid transparent;
    border-radius: 50%;
    line-height: normal;
    position: relative;
    text-align: center;
    z-index: 2;
    transition: all 0.1s linear 0s;
}

.wfWizard li.done .step:after {
     content: '\f00c';
     font-family: FontAwesome;
     font-weight: 400;
     height: 26px;
     width: 26px;
     left: 50%;
     top: 50%;
     margin-left: -13px;
     margin-top: -13px;
     background: #559c63;
     border-radius: 9999px;
     display: block;
     font-size: 20px;
     line-height: 26px;
     position: absolute;
     /*text-align: center;*/
 }

.wfWizard li.reject .step:after {
    content: '\f00d';
    font-family: FontAwesome;
    font-weight: 400;
    height: 26px;
    width: 26px;
    left: 50%;
    top: 50%;
    margin-left: -13px;
    margin-top: -13px;
    background: #D62D2A;
    border-radius: 9999px;
    display: block;
    font-size: 20px;
    line-height: 26px;
    position: absolute;
    /*text-align: center;*/
}

.wfWizard li.active .step, .wfWizard li.active.complete .step {
    background: #0091D9;
    color: #FFF;
    font-weight: bold;
    padding: 8px 14px;
    font-size: 15px;
    border-radius: 50%;
    border: 5px solid #55606E;
}

.wfWizard li.current .step, .wfWizard li.current.complete .step {
    background: #0091D9;
}

.wfWizard li.reject .step {
    background: #D62D2A;
}

.wfWizard li.done .step, .wfWizard li.done.complete .step {
    background: #559c63;
}

.wfWizard li.terminate .step, .wfWizard li.terminate.complete .step {
    background: #dd4747;
}

.wfWizard li.future .step, .wfWizard li.future.complete .step {
    background: #B2B5B9;
}

.wfWizard li.complete .title, .wfWizard li.active .title {
    color: #2B3D53;
}

.wfWizard li.complete .wfheader, .wfWizard li.active .wfheader {
    color: #2B3D53;
}

.wfWizard li .title {
    color: #756F6F;
    display: block;
    font-size: 13px;
    line-height: 15px;
    max-width: 100%;
    position: relative;
    table-layout: fixed;
    text-align: center;
    top: 25px;
    word-wrap: break-word;
    z-index: 104;
}

.wfWizard li .wfheader {
    color: #756F6F;
    display: block;
    /*font-size: 13px;*/
    line-height: 15px;
    max-width: 100%;
    position: relative;
    table-layout: fixed;
    text-align: center;
    top: -20px;
    word-wrap: break-word;
    z-index: 104;
}

.uploadify {
    position: relative;
    display: inline-block;
}
.uploadify-button {
    background-color: #3276b1;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    border: 1px solid #3276b1;
    color: #FFF;
    font: 12px "Microsoft YaHei", Arial, Helvetica, Sans-Serif;
    text-align: center;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    width: 100%;
    height: 30px;
}
.uploadify:hover .uploadify-button,.uploadify .uploadify-button:hover {
    background-color: #296191;

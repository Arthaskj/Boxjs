/*
 * TREE
 */
.tree li span {
    border: 1px dotted #999;
    border-radius: 5px;
    display: inline-block;
    padding: 3px 8px;
    text-decoration: none;
    -webkit-transition: color .2s ease .1s,background-color .2s ease .1s,border-color .3s ease .2s;
    transition: color .2s ease .1s,background-color .2s ease .1s,border-color .3s ease .2s;
}

.tree ul li span i {
    width: 14px;
    font-size: 18px;
    vertical-align: bottom;
    margin-right: 5px;
}

.tree ul li span i.fa-minus-circle {
    color: cornflowerblue;
}

.tree ul li span i.fa-plus-circle {
    color: mediumorchid;
}

.tree ul li span i.fa-plus-square, ul li span i.fa-check-square-o {
    color: green;
}

.tree ul li span i.fa-minus-square-o {
    color: red;
}

.tree ul li span i.fa-square-o {
    color: black;
}
.tree {
    min-height: 20px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
}

.tree li {
    list-style-type: none;
    margin: 0;
    padding: 5px;
    position: relative;
}

.tree ul ul li:hover {
    background: rgba(0,0,0,.015);
}

.tree li:before, .tree li:after {
    content: '';
    left: -20px;
    position: absolute;
    right: auto;
}

.tree li:before {
    border-left: 1px solid #999;
    bottom: 50px;
    height: 100%;
    top: -6px;
    width: 1px;
    -webkit-transition: border-color .1s ease .1s;
    -moz-transition: border-color .1s ease .1s;
    -o-transition: border-color .1s ease .1s;
    transition: border-color .1s ease .1s;
}

.tree li:after {
    border-top: 1px solid #999;
    height: 20px;
    top: 18px;
    width: 25px;
}

.dropdown-menu > li:before, .dropdown-menu > li:after {
    content: none;
    left: auto;
}

.tree li .dropdown-header {
    color: black;
    font-size: 13px;
    font-weight: bold;
}

.dropdown-menu > li {
    padding: 0px;
}

.tree li > .treenode {
    width: 100%;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-bottom-color: rgb(105, 101, 101);
    border-bottom-style: dotted;
    border-bottom-width: 1px;
    border-radius: 5px;
    display: inline-block;
    /*padding:3px 8px;*/
    text-decoration: none;
    -webkit-transition: color .2s ease .1s,background-color .2s ease .1s,border-color .3s ease .2s;
    -moz-transition: color .2s ease .1s,background-color .2s ease .1s,border-color .3s ease .2s;
    -o-transition: color .2s ease .1s,background-color .2s ease .1s,border-color .3s ease .2s;
    transition: color .2s ease .1s,background-color .2s ease .1s,border-color .3s ease .2s;
}

.tree li.parent_li > span {
    cursor: pointer;
    padding: 7px;
}

.tree li.parent_li > .treenode {
    /*cursor:pointer;*/
    padding: 7px;
}

.tree > ul > li:before, .tree > ul > li::after {
    border: 0;
}

.tree li:last-child::before {
    height: 25px;
}

.tree li.parent_li > span:hover, .tree li.parent_li > span:hover + ul li span {
    background: #eee;
    border: 1px solid #94a0b4;
    color: #000;
}

.tree li.parent_li > .treenode:hover, .tree li.parent_li > .treenode:hover + ul li > .treenode {
    border-bottom-color: rgb(105, 101, 101);
    border-bottom-style: dotted;
    border-bottom-width: 1px;
    color: #000;
}

.tree > ul {
    padding-left: 0px;
}

.tree ul ul {
    padding-left: 34px;
    padding-top: 5px;
}

.tree ul div ul {
    padding-left: 5px;
    padding-top: 2px;
    width: 200px;
    overflow: auto;
    max-height: 150px;
}

.tree li.parent_li > span.pselected {
    border-color: green;
    background-color: #eeffff;
}

.tree li.parent_li > span.pselected:hover {
    border-color: green;
}

.tree li.parent_li > span:hover {
    background-color: #FCC937;
    border: 1px solid #C67605;
    /*color: #FFF;*/
}

.tree li.parent_li > .treenode:hover {
    border-bottom-color: rgb(105, 101, 101);
    border-bottom-style: dotted;
    border-bottom-width: 1px;
}


.tree li.parent_li > span:hover + ul li::before {
    border-left-color: #F89406;
}

.tree li.parent_li > .treenode:hover + ul li::before {
    border-left-color: #FAA937;
}

.tree li.parent_li > span:hover + ul li::after {
    border-top-color: #F89406;
}

.tree li.parent_li > .treenode:hover + ul li::after {
    border-top-color: #FAA937;
}

.tree li.parent_li > span:hover + ul li span.pselected {
    background: #FDDFB3 !important;
    border: 1px solid green;
    color: #000;
}

.tree li.parent_li > span:hover + ul li span {
    background: #FDDFB3 !important;
    border: 1px solid #FAA937;
    color: #000;
}

.tree li.parent_li > .treenode + ul li > .treenode {
    color: #000;
}

.tree li.parent_li > .treenode:hover + ul li > .treenode {
    border-bottom-color: rgb(105, 101, 101);
    border-bottom-style: dotted;
    border-bottom-width: 1px;
    color: #000;
}




